{
    "name": "server",
    "version": "0.0.0",
    "main": "server.js",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && npm run start",
        "seed": "node scripts/seedDB.js",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.0.0",
        "express": "^4.16.4",
        "google-maps-react": "^2.0.2",
        "http-errors": "~1.6.3",
        "if-env": "1.0.4",
        "is-empty": "^1.2.0",
        "jade": "~1.11.0",
        "jsonwebtoken": "^8.5.1",
        "jwt-decode": "^2.2.0",
        "moment": "^2.24.0",
        "mysql2": "^1.6.5",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "react-google-maps": "^9.4.5",
        "react-images": "^1.0.0",
        "react-redux": "^7.1.0",
        "react-slick": "^0.24.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "semantic-ui-react": "^0.87.2",
        "sequelize": "^5.8.9",
        "sequelize-cli": "^5.5.0",
        "socket.io": "^2.4.0",
        "slick-carousel": "^1.8.1",
        "uuid": "^3.3.2",
        "validator": "^10.9.0"
    },
    "devDependencies": {
        "concurrently": "^4.1.0",
        "nodemon": "^1.18.7"
    }
}
